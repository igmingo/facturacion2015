	<queryString>
		<![CDATA[SELECT facturacion2015.facturas.`dirCorreo`,
	facturacion2015.facturas.`dirFactura`,
	facturacion2015.facturas.cobrada,
	facturacion2015.facturas.`nombreCliente`,
	facturacion2015.clientes.nif,
	facturacion2015.facturas.`impTotal`,
	facturacion2015.facturas.`dirEnvio`,
	facturacion2015.facturas.`porcDescuento`,
	facturacion2015.facturas.`impRecargo`,
	facturacion2015.facturas.`clienteId`,
	facturacion2015.facturas.`porcRecargoEquivalencia`,
	facturacion2015.facturas.`impIva`,
	facturacion2015.facturas.id,
	facturacion2015.facturas.numero,
	facturacion2015.facturas.fecha
FROM facturacion2015.facturas,  facturacion2015.clientes 
WHERE 
facturacion2015.clientes.id =  facturacion2015.facturas.`clienteId`
AND
	 facturacion2015.facturas.id = '1']]>
